  if (mmcore.GetCookie('PersonalizedSortTnT', true) === '' || !(/&rd=f/.test(mmcore.GetCookie('PersonalizedSortTnT', true)))) mmcore.SetCookie('PersonalizedSortTnT', csort_vars[a(2)] + '&rd=f', 365, true, true);
})();

